pipeline {
   agent any
 stages {
        stage('git integration') {
            steps {
                git branch:'master', url:'https://github.com/Chol-ui/git_task_integration.git'
                echo 'git integrated'
            }
        }
        
        stage('build') {
            steps {
                script {
                    docker.build('imag20')
            } 
       } 
   } 

   stage('container') {
           steps {
              script {
                    docker.build -t imag20 .
                        docker run -d -p 8017:80 imag20
              }
           }
        }
           stages {
        stage('Checkout') {
            steps {
                // Checkout SCM (Source Control Management) sources
                checkout scm
            }
        }
        // Additional stages can follow here
    }
    // Post-build actions, notifications, etc.
   }
 }
} 
