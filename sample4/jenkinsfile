pipeline {
   agent any
 stages {
        stage('git integration') {
            steps {
                git branch:'master', url:''
                echo 'git cloned'
            }
        }
        
        stage('pull') {
            steps {
                path: C:\Git\jenkins_task1
                bat 'git pull https://github.com/Chol-ui/jenkins_task1.git'
                echo 'sucessfully pulled'
            } 
       } 
   } 
}            
 




